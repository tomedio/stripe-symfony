{
  "name": "tomedio/stripe-symfony",
  "version": "0.2.2",
  "description": "Stripe integration for Symfony applications with Doctrine and API Platform",
  "type": "symfony-bundle",
  "license": "MIT",
  "keywords": [
    "symfony",
    "stripe",
    "payment",
    "subscription",
    "doctrine",
    "api-platform"
  ],
  "homepage": "https://tomedio.github.io/stripe-symfony/",
  "authors": [
    {
      "name": "Tomedio",
      "homepage": "https://github.com/tomedio"
    }
  ],
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": "^6.0|^7.0",
    "symfony/dependency-injection": "^6.0|^7.0",
    "symfony/config": "^6.0|^7.0",
    "symfony/http-kernel": "^6.0|^7.0",
    "symfony/yaml": "^6.0|^7.0",
    "doctrine/orm": "^2.10",
    "doctrine/doctrine-bundle": "^2.5",
    "api-platform/core": "^3.0",
    "stripe/stripe-php": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^6.0|^7.0",
    "symfony/browser-kit": "^6.0|^7.0",
    "symfony/css-selector": "^6.0|^7.0"
  },
  "autoload": {
    "psr-4": {
      "Tomedio\\StripeBundle\\": "src/StripeBundle/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tomedio\\StripeBundle\\Tests\\": "tests/StripeBundle/"
    }
  },
  "support": {
    "issues": "https://github.com/tomedio/stripe-symfony/issues",
    "source": "https://github.com/tomedio/stripe-symfony"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "endpoint": [
        "https://raw.githubusercontent.com/tomedio/flex-recipes/flex/main/index.json",
        "flex://defaults"
      ]
    }
  }
}
